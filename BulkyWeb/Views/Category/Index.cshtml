@model List<Category>
<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <tr>
        <th>
            Category Name 
        </th>
        <th>
            Display Order
        </th>
    </tr>
        <tbody>
            @foreach(var item in Model){
            <tr>
                <td>@item.CategoryName</td>
                <td>@item.DisplayOrder</td>
            </tr>
            }
            
        </tbody>
</table>